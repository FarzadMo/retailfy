  "bugs": {
    "url": "https://github.com/fred1525/retailfy/issues"
  },
  "homepage": "https://github.com/fred1525/retailfy#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.9",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "multer": "^1.4.2",
    "mysql2": "^1.7.0",
    "sequelize": "^5.19.6",
    "sequelize-cli": "^5.5.1",
    "aws-sdk": "2.x",
    "ejs": "2.x"
  },
